<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity KemrReservation.
    -->
    <changeSet id="20220831090433-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="kemr_patient_id"
                                 baseTableName="kemr_reservation"
                                 constraintName="fk_kemr_reservation__kemr_patient_id"
                                 referencedColumnNames="id"
                                 referencedTableName="kemr_patient"/>

        <addForeignKeyConstraint baseColumnNames="kemr_doctor_id"
                                 baseTableName="kemr_reservation"
                                 constraintName="fk_kemr_reservation__kemr_doctor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="kemr_doctor"/>
    </changeSet>
</databaseChangeLog>
