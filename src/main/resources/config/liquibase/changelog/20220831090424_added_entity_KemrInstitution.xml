    <!--
        Added the entity KemrInstitution.
    -->
    <changeSet id="20220831090424-1" author="jhipster">
        <createTable tableName="kemr_institution">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="kemr_institution_id" type="varchar(20)">
                <constraints nullable="false" unique="true" uniqueConstraintName="ux_kemr_institution__kemr_institution_id" />
            </column>
            <column name="kemr_institution_password" type="varchar(60)">
                <constraints nullable="false" />
            </column>
            <column name="kemr_institution_name" type="varchar(20)">
                <constraints nullable="false" />
            </column>
            <column name="kemr_institution_department" type="varchar(10)">
                <constraints nullable="true" />
            </column>
            <column name="kemr_institution_position" type="varchar(10)">
                <constraints nullable="true" />
            </column>
            <column name="kemr_institution_manager" type="varchar(20)">
                <constraints nullable="false" />
            </column>
            <column name="kemr_institution_cellphone" type="varchar(20)">
                <constraints nullable="false" />
            </column>
            <column name="kemr_institution_email" type="varchar(40)">
                <constraints nullable="false" />
            </column>
            <column name="kemr_institution_website" type="varchar(40)">
                <constraints nullable="true" />
            </column>
            <column name="kemr_institution_agree" type="${datetimeType}">
                <constraints nullable="false" />
            </column>
            <column name="kemr_institution_status" type="varchar(1)">
                <constraints nullable="false" />
            </column>
            <column name="kemr_institution_modification" type="${datetimeType}">
                <constraints nullable="true" />
            </column>
            <column name="kemr_institution_withdrawal" type="${datetimeType}">
                <constraints nullable="true" />
            </column>
            <column name="kemr_institution_landline" type="varchar(20)">
                <constraints nullable="true" />
            </column>
            <column name="kemr_institution_fail" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="kemr_institution_failtime" type="${datetimeType}">
                <constraints nullable="true" />
            </column>
            <column name="kemr_institution_number" type="varchar(10)">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
        <dropDefaultValue tableName="kemr_institution" columnName="kemr_institution_agree" columnDataType="${datetimeType}"/>
        <dropDefaultValue tableName="kemr_institution" columnName="kemr_institution_modification" columnDataType="${datetimeType}"/>
        <dropDefaultValue tableName="kemr_institution" columnName="kemr_institution_withdrawal" columnDataType="${datetimeType}"/>
        <dropDefaultValue tableName="kemr_institution" columnName="kemr_institution_failtime" columnDataType="${datetimeType}"/>
    </changeSet>

