<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity KemrMedicalTreatment.
    -->
    <changeSet id="20220831090426-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="kemr_patient_id"
                                 baseTableName="kemr_medical_treatment"
                                 constraintName="fk_kemr_medical_treatment__kemr_patient_id"
                                 referencedColumnNames="id"
                                 referencedTableName="kemr_patient"/>

        <addForeignKeyConstraint baseColumnNames="kemr_docter_id"
                                 baseTableName="kemr_medical_treatment"
                                 constraintName="fk_kemr_medical_treatment__kemr_docter_id"
                                 referencedColumnNames="id"
                                 referencedTableName="kemr_doctor"/>

        <addForeignKeyConstraint baseColumnNames="kemr_diagnosis_id"
                                 baseTableName="kemr_medical_treatment"
                                 constraintName="fk_kemr_medical_treatment__kemr_diagnosis_id"
                                 referencedColumnNames="id"
                                 referencedTableName="kemr_diagnosis"/>

        <addForeignKeyConstraint baseColumnNames="kemr_treatment_id"
                                 baseTableName="kemr_medical_treatment"
                                 constraintName="fk_kemr_medical_treatment__kemr_treatment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="kemr_treatment"/>

        <addForeignKeyConstraint baseColumnNames="kemr_prescription_id"
                                 baseTableName="kemr_medical_treatment"
                                 constraintName="fk_kemr_medical_treatment__kemr_prescription_id"
                                 referencedColumnNames="id"
                                 referencedTableName="kemr_prescription"/>
    </changeSet>
</databaseChangeLog>
